<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="386.0" prefWidth="452.0" spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.example.loginui.BudgetController">

    <Label text="Budget Name:" />
    <TextField fx:id="budgetNameField" />

    <Label text="Income:" />
    <TextField fx:id="incomeField" />

    <Label text="Income Start Date:" />
    <DatePicker fx:id="startDatePicker" />

    <Label text="Income End Date:" />
    <DatePicker fx:id="endDatePicker" />

    <Button onAction="#budgetadd" text="Save" />
</VBox>
